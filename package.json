    "lucide-react": "^0.263.1",
    "react-markdown": "^8.0.7",
    "remark-gfm": "^3.0.1",
    "rehype-highlight": "^6.0.0",
    "rehype-raw": "^6.1.1",
    "@monaco-editor/react": "^4.5.1",
    "tailwindcss": "^3.3.0",
    "@headlessui/react": "^1.7.15",
    "@tailwindcss/typography": "^0.5.9",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "nanoid": "^4.0.2"
